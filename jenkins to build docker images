// SCRIPTED
//DECARATIVE
pipeline {
    // agent any
    agent{docker{image 'maven:3.6.3'}}
stage {
	  stage('Build') {
	 steps {
        sh 'mvn --version'	
        echo "Build"
	 }
    }
	 stage('Test') {
		steps {
            echo "Test"
	} 
      }
      stage('Integration Test') {
		steps {
            echo "integration Test"
	 }
        }
 }
}